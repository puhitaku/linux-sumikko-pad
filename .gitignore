/fs/unicode/utf8data.h
/kernel/config_data.h
/kernel/kheaders.md5
/lib/crc32table.h
/lib/gen_crc32table
/lib/oid_registry_data.c
/net/wireless/shipped-certs.c
/security/selinux/av_permissions.h
/security/selinux/flask.h
/usr/gen_init_cpio
