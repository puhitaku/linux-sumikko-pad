&rkisp_vir0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&cif_clk>;
	port {
		#address-cells = <1>;
		#size-cells = <0>;

		isp_in0: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&dphy0_out>;
		};
		isp_in1: endpoint@1 {
			reg = <1>;
			remote-endpoint = <&dphy1_out>;
		};		
		isp_in2: endpoint@2 {
			reg = <1>;
			remote-endpoint = <&dphy2_out>;
		};
	};
};

