	status = "okay";

	ts@40 {
		status = "okay";
		compatible = "GSL,GSL3673_800X1280";
		reg = <0x40>;
		irq_gpio_number = <&gpio0 RK_PC1 IRQ_TYPE_LEVEL_LOW>;
		rst_gpio_number = <&gpio0 RK_PB4 GPIO_ACTIVE_HIGH>;
	};
	sensor7660@1d {
		compatible = "gs_sc7660"; 
		reg = <0x1d>; 
		type = <SENSOR_TYPE_ACCEL>; 
		//irq-gpio = <&gpio2 GPIO_B2 IRQ_TYPE_EDGE_FALLING>; 
		irq_enable = <0>; 
		poll_delay_ms = <10>; 
		layout = <7>; //<7>;//<5>; 
		reprobe_en = <1>;
		revert_x = <0>;
		revert_y = <0>;
		revert_z = <0>;
	};

