&io_domains {
	status = "okay";

	vccio1-supply = <&vcc_3v0>;
	vccio2-supply = <&vccio_sd>;
	vccio3-supply = <&vcc2v8_dvp>;
	vccio4-supply = <&vcc_3v0>;
