&pinctrl {
	cam {
		camera_rst: camera-rst {
			rockchip,pins =
				/* back camra reset */
				<4 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		camera_af_en: camera-af {
			rockchip,pins =
				/* back camra reset */
				<4 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		flash_led_gpios: flash-led {
			rockchip,pins =
				/* flash led enable */
				<4 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	tp {
		tp_gpio: tp-gpio {
			rockchip,pins = <0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>,
					<0 RK_PC2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins = <3 RK_PA1 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	lcd {
		lcd_rst_gpio: lcd-rst-gpio {
			rockchip,pins = <3 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		lcd_enable_gpio: lcd-enable-gpio {
			rockchip,pins = <3 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};

	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		soc_slppin_gpio: soc_slppin_gpio {
			rockchip,pins =
				<0 RK_PA2 RK_FUNC_GPIO &pcfg_output_low>;
		};

		soc_slppin_slp: soc_slppin_slp {
			rockchip,pins =
				<0 RK_PA2 RK_FUNC_1 &pcfg_pull_none>;
		};

		soc_slppin_rst: soc_slppin_rst {
			rockchip,pins =
				<0 RK_PA2 RK_FUNC_2 &pcfg_pull_none>;
		};
	};

	sensor {
		sensor_gpio: sensor-gpio {
			rockchip,pins = <3 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <0 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		
		wifi_vbat: wifi-vbat {
			rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};		
	};

	usb2 {
		host_vbus_drv: host-vbus-drv { 
				rockchip,pins =
						<3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
		
	vcc_sd {
		vcc_sd_h: vcc-sd-h {
			rockchip,pins = <0 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		wifi_host_wake_irq: wifi-host-wake-irq {
			rockchip,pins = <0 RK_PC4 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	wireless-bluetooth {
		uart1_gpios: uart1-gpios {
			rockchip,pins = <2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
        	bt_reset_gpio: bt-reset-gpio {
                	rockchip,pins = <0 RK_PC1 RK_FUNC_GPIO &pcfg_pull_down>;
        	};

        	bt_wake_gpio: bt-wake-gpio {
                	rockchip,pins = <0 RK_PC3 RK_FUNC_GPIO &pcfg_pull_down>;
        	};      

        	bt_irq_gpio: bt-irq-gpio {
                	rockchip,pins = <0 RK_PB7 RK_FUNC_GPIO &pcfg_pull_down>;
		}; 		
	};
};

